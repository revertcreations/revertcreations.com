{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "format": "prettier --write \"{app,config,resources,tests}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"{app,config,resources,tests}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"*.{js,json,md}\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "laravel-mix": "^6.0.49",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.6"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "chokidar": "^3.5.3",
    "gsap": "^3.13.0",
    "node": "^21.2.0"
  }
}
